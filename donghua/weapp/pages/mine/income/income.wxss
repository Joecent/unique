/* pages/mine/Withdraw/Withdraw.wxss */

.container_top {
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  align-items: center;
  height: 240rpx;
  line-height: 60rpx;
}
.container_top_item{
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
.container_top_item2{
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  color: #aaa
}
.container_method {
  display: flex;
  flex-direction: column;
  align-items: center;
  position: relative;
}

.container_method_popup{
  font-size: 32rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  width: 650rpx;
  border-top: 1px solid #D5D4D4;
  height: 88rpx;
  background-color: white;
   box-shadow: 0px 5px 13px 0px rgba(153, 153, 153, 0.09);
}

.container_method_image {
  width: 40rpx;
  height: 30rpx;
  position: absolute;
  right: 110rpx;
  top: 29rpx;
}

.container_method_concrete {
  font-size: 32rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  width: 650rpx;
  border-top: 1px solid #D5D4D4;
  height: 88rpx;
  border-radius: 10rpx ;
  
}

.container_method_import {
  margin-top: 20rpx;
  /* border-top: 1px solid #D5D4D4; */
  display: flex;
  flex-direction: column;
  align-items: center;
  background-color: white;
  width: 650rpx;
  padding-bottom: 80rpx;
  border-radius: 10rpx;
  box-shadow: 0px 5px 13px 0px rgba(153, 153, 153, 0.09);
}

.container_method_import_text {
  display: flex;
  flex-direction: row;
  align-items: center;
  border-bottom: 1px solid #d5d4d4;
  margin-top: 60rpx;
  padding-bottom: 16rpx;
  text-justify: center;
  width: 530rpx;
}

.container_instruction {
  width: 650rpx;
  display: flex;
  flex-direction: column;
  margin-top: 80rpx;
  font-size: 18rpx;
}

.container_instruction image {
  width: 50rpx;
  height: 50rpx;
}

.container_instruction_last {
  display: flex;
  align-items: center;
  margin-top: 15rpx;
}

.container_instruction_last_image {
  background: url(data:image/png;base64,) no-repeat 0rpx center;
  background-size: 40rpx auto;
}

.container_instruction_last_image.active {
  background: url(data:image/png;base64,) no-repeat 0rpx center;
  background-size: 40rpx auto;
}

.spacing {
  margin-top: 15rpx;
}

.btn {
  margin-top: 65rpx;
  margin-bottom: 95rpx;
  
  border-radius: 40rpx;
  font-size: 36rpx;
  height: 80rpx;
  width: 650rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}

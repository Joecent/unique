<view class='container'>
  <view style='background-color:{{backgroundColor}};color:#ffffff;' class='container_top'>
    <image src='{{avatarUrl}}'></image>
    <view class='container_top_content'>
      <text style='font-size:32rpx;'>{{name}}</text>
      <text style='font-size:28rpx;margin-top:15rpx;'>{{phone}}</text>
    </view>
  </view>

  <view class='container_earnings'>
    <text>我的收益</text>
    <view>
      <text style='font-size:26rpx;'>￥{{allIncome}}</text>
      <text style='color:#057AFE;margin-left:23rpx;' bindtap='jumoWithdraw'>提现</text>
    </view>
  </view>

  <view bindtap='jumoOrCode' style='margin-top:16rpx;' class='container_earnings'>
    <text>我的二维码</text>
    <view class='container_earnings_image'>
      <icon class='iconfont icon-icon1460187932551' style='margin-right:16rpx;'></icon>
      <image src='/image/one.png' class='container_earnings_image-1'></image>
    </view>
  </view>

  <view class='container_detail'>
    <view bindtap='jumpSelesDetails' style='margin-top:16rpx;' class='container_earnings'>
      <text>收益明细</text>
      <view class='container_earnings_image'>
        <image src='/image/one.png' class='container_earnings_image-1'></image>
      </view>
    </view>

    <view hidden='{{allHid}}' wx:for='{{list}}' class='container_item'>
    <view wx:if='{{item.refund_status==1}}' class='container_item_content'>
      <view>
        <view style='display: flex;flex-direction: row;align-items: baseline;'>
          <view style='width:140rpx;margin-bottom:10rpx; overflow: hidden;text-overflow: ellipsis;white-space: nowrap'>{{item.userName}}</view>
          <text>{{item.status==2?'购买商品':'金额提现'}}</text>
          <text style='margin-left:30rpx;' wx:if='{{item.cash_type==1}}'>微信提现</text>
          <text style='margin-left:30rpx;' wx:if='{{item.cash_type==2}}'>支付宝</text>
          <text style='margin-left:30rpx;' wx:if='{{item.cash_type==3}}'>银行卡</text>
        </view>
      </view>
      <text style="color:{{item.status==2?backgroundColor:''}}">{{item.status==2?'+':'-'}}￥{{item.sale_money}}</text>
    </view>

    <view wx:if='{{item.refund_status==2}}' class='container_item_content'>
      <view>
        <view style='display: flex;flex-direction: row;align-items: baseline;'>
          <view style='width:140rpx;margin-bottom:10rpx; overflow: hidden;text-overflow: ellipsis;white-space: nowrap'>{{item.userName}}</view>
          <text>提现失败</text>
          <text style='margin-left:40rpx;' wx:if='{{item.cash_type==1}}'>微信提现</text>
          <text style='margin-left:40rpx;' wx:if='{{item.cash_type==2}}'>支付宝</text>
          <text style='margin-left:40rpx;' wx:if='{{item.cash_type==3}}'>银行卡</text>
          <view class='looked' bindtap='showStratum' style='margin-left:30rpx;background-color: {{backgroundColor}}' data-user="{{item}}">查看原因</view>
        </view>
      </view>
      <text style="color:{{item.status==2?backgroundColor:''}}">{{item.status==2?'+':'-'}}￥{{item.sale_money}}</text>
    </view>
  </view>
    <view hidden='{{moreHid}}' bindtap='jumpSelesDetails' class='container_detail_bottom'>
      <text>查看更多</text>
      <image class='container_detail_bottom_image' src='/image/refund/choose.png'></image>
    </view>

  </view>

 <view wx:if="{{list.length ==0}}">
      <view class='none'>
        <view class='none_image'>
          <image src='/image/none.png'></image>
        </view>
        <view class='tips fs24'>这里什么都没有哦~</view>
      </view>
    </view>

  <view catchtouchmove="ture" class="bg" bindtap='hideview' style='display:{{display}}'></view>
  <view catchtouchmove="ture" class="classshow" style='display:{{display}}'>
    <view class='classshow_content'>
      <view class='classshow_content_top'>拒绝理由</view>
      <view class='classshow_content_text'>{{extra}}</view>
    </view>
  </view>









</view>
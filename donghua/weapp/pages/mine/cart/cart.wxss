  /* line-height: 100rpx; */
  color: #484848;
  float: right;
}
.base {
  width: 100%;
  height: 100rpx;
  background-color: #f8f8f8;
}

.inquiry {
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
}

.goodsList .art {
  width: 100%;
  height: auto;
  background-color: #000;
  opacity: 0.8;
  text-align: center;
  color: #fff;
  position: absolute;
  top: 0;
  left: 0;
  font-size: 20rpx;
  padding: 0;
  display: none;
}

.goodsList .block {
  display: block;
}

.title_name {
  width: 100%;
  height: auto;
  font-size: 40rpx;
  color: #484848;
  margin-bottom: 20rpx;
}

.select {
  width: 100%;
  height: auto;
  position: inherit;
  border-top: none;
}

.select .length {
  color: #787878;
  font-size: 30rpx;
  line-height: 100rpx;
  margin-right: 30rpx;
}

.details {
  flex-grow: 1;
  height: auto;
  overflow: hidden;
  padding: 31rpx 5rpx;
  position: relative;
}

.details image {
  width: 150rpx;
  height: 154.1rpx;
  display: block;
  float: left;
  /* margin-right: 20rpx; */
}

.details .name_author {
  padding-left: 30rpx;
  width: 60%;
  height: auto;
  float: left;
}

.details .name_author text {
  width: 100%;
  height: auto;
  float: left;
  overflow: hidden;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  color: #999;
  margin-bottom: 10rpx;
}

.details .name_author .title_details {
  width: 100%;
  height: auto;
  float: left;
  overflow: hidden;
  display: -webkit-box;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
  color: #484848;
}


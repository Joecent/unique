.nav_right .mink:nth-child(1) .minl {
  background: #fff;
}

minl-bg {
  background: url(data:image/png;base64,);
  background-size: 100% 100%;
  background-repeat: no-repeat;
  width: 230rpx;
  height: 58rpx;
}

.nav-right-list {
  /* background: #fff; */
  margin-bottom: 10rpx;
}

.nav-right-list-titles {
  padding:20rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  background: #fff ;
}

.nav_right_img image {
  width: 220rpx;
  height: 227rpx;
  display: block;
}

.nav-right-conten {
  display: flex;
  border-bottom: 1px solid #e6e6e6;
  /* margin-top:20rpx; */
  background: #fff;
}

.nav_right_goods_title {
  padding-top: 20rpx;
  color: #333;
  overflow: hidden;
}

.nav_right_lf {
  width: 36%;
  padding-right: 20rpx;
  padding-left: 30rpx;
  display: -webkit-box;
  word-break: break-all;
  text-overflow: ellipsis;
  overflow: hidden;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
}

.nav-btns {
  margin-top: 80rpx;
}

.commodity_screen {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: #000;
  opacity: 0.5;
  overflow: hidden;
  z-index: 9;
  color: #fff;
}

/* 弹出框 */

.maskLayer {
  position: fixed;
  z-index: 10;
  width: 100%;
  height: 100%;
  bottom: 0;
  background: rgba(0, 0, 0, 0.5);
}

.choose {
  position: fixed;
  z-index: 103;
  bottom: 0rpx;
  width: 100%;
  height: 80%;
}

.specs {
  width: 100%;
  height: 100%;
  position: relative;
}

.specs_info {
  background-color: white;
  position: absolute;
  top: 100rpx;
  bottom: 0;
  left: 0;
  padding: 30rpx 0;
  width: 100%;
  color: #333;
}

.image {
  position: absolute;
  left: 40rpx;
  top: 0rpx;
  z-index: 50;
}

.image image {
  width: 200rpx;
  height: 200rpx;
  border-radius: 10rpx;
  background-color: white;
  margin-top: 20rpx;
}

.spec_name {
  color: #222;
  font-weight: bold;
  padding-left: 40rpx;
  margin-top: 70rpx;
}

.spec_value {
  display: flex;
  justify-content: flex-start;
  flex-wrap: wrap;
  margin-top: 40rpx;
  padding: 0 60rpx 0 40rpx;
}

.value {
  margin-right: 60rpx;
  background-color: #e4e4e4;
  padding: 2rpx 20rpx;
  border-radius: 6rpx;
  margin-bottom: 30rpx;
}

.price_score {
  padding: 0 60rpx 0 250rpx;
  justify-content: space-between;
  height: 50rpx;
  line-height: 50rpx;
}

.spec_price {
  margin-right: 30rpx;
  display: flex;
}

.spec_market_prices {
  color: #aaa;
  text-decoration: line-through;
  margin-left: 20rpx;
  font-size: 28rpx;
}

.score {
  color: #aaa;
}

.on {
  color: white;
}

.buttons {
  position: absolute;
  bottom: 0rpx;
  width: 100%;
}

.imform {
  border-radius: 0;
  color: white;
}

.nums {
  /* position: relative; */
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 0 5%;
}

.num {
  line-height: 60rpx;
}

.num .noclick {
  color: #a3a2a2;
}

.num text {
  width: 60rpx;
  height: 60rpx;
  text-align: center;
  float: left;
  border: 1px solid #e6e6e6;
}

.num input {
  width: 60rpx;
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
  display: block;
  float: left;
  border: 1px solid #e6e6e6;

}

.ss {
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
  /* margin-top: 10rpx; */
}

.bottomMenu{
  height: 120rpx;
  width: 100%;
  position: fixed;
  bottom: 0;
  display: flex;
  flex-direction: row;
  background-color: white;
  box-shadow: 6rpx 6rpx 6rpx 6rpx #aaa;
  z-index: 102;
}
.bottomMenu-item{
  width: 25%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}

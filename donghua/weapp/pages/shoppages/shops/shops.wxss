/* pages/shoppages/shops/shops.wxss */

.shops-top {
  padding: 0 20rpx; 
  display: flex;
  justify-content: space-between;
  align-items: center;

  /* background: #fff; */
}

.section {
  flex: 6;
  position: relative;
  padding: 20rpx 0;
}

.shops-fenlei-img image {
  width: 36rpx;
  height: 32rpx;
}

.section input {
  /* width: 95%; */
  background: #fff;
  margin: 0 auto;
  height: 68rpx;
  line-height: 68rpx;
  border-radius: 10rpx;
  padding-left: 80rpx;
  /* border: 1px solid #d5d4d4; */
}

.section image {
  width: 30rpx;
  height: 30rpx;
  position: absolute;
  top: 40rpx;
  left: 30rpx;
  z-index: 5;
}

.shops-fenlei, .shops-lb {
  margin-right: 14rpx;
  text-align: center;
  color: #666;
  font-size: 0;
  margin-top: 4rpx;
}

.shops-fenlei text, .shops-lb text {
  margin-top: 6rpx;
}

/* 商品 */

.shops-conten {
  width: 100%;
  background: #fff;
}

.shops-conten-top {
  padding: 26rpx 5%;
  display: flex;
  justify-content: space-between;
  /* border-bottom: 1px solid #D5D4D4; */
  border-top: 1px solid #e6e6e6;
  border-bottom: 1px solid #e6e6e6;
  color: #333;
}

.shops-conten-img image {
  width: 14rpx;
  height: 20rpx;
}

/*总体主盒子*/

.container {
  position: relative;
  width: 100%;
  color: #939393;
}

/*左侧栏主盒子*/

.nav_left {
  display: inline-block;
  width: 100%;
  /* height: 100%; *//* background: #fff; */
  text-align: center;
  /* position: fixed;  */
  left: -100px;
  top: 0;
  /* border-top: 1rpx solid #dedede; */
}

/*左侧栏list的item*/

.nav_left .nav_left_items {
  background: #ebeaea;
  height: 120rpx;
  line-height: 120rpx;
  /* padding: 15rpx 0; *//* border-bottom: 1px solid #dedede; */
  color: #999;
  font-weight: 700;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

/*左侧栏list的item被选中时*/

.nav_left .nav_left_items.active {
  background: #fff;
  color: #333;
  box-sizing: border-box;
}

/*右侧栏主盒子*/

.scroll_right {
  position: fixed;
  /* top: 50px; */
  left: 200rpx;
  overflow: auto;
  flex: 1;
  width: 100%;
  height: 100%;
  /* padding: 0 20rpx; */
  box-sizing: border-box;
}

.mink::after {
  display: block;
  content: '';
  clear: both;
}

.jiul, .jiul image {
  width: 100%;
  height: 170rpx;
}

.minl {
  /* color: #777; */
  text-align: center;
  /* height: 100rpx;
  line-height: 100rpx; *//* padding-left: 10rpx; *//* background: #fff; */
  width: 100%;
  padding: 20rpx 0;
  /* position: relative; */
}

/* .minl-bg image{
  width: 230rpx;
  height: 58rpx;
  margin-top: 22rpx;
} */

.minl-bg {
  /* background: url(data:image/png;base64,) no-repeat center; */
  width: 230rpx;
  height: 58rpx;
  background-size: cover;
  margin: 0 auto;
  line-height: 58rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.minl-bg1{
  margin: inherit;
}

.minl-title {
  /* position: absolute;
  top: 0;
  left: 47%; */
  position: relative;
  width: 70%;
  display: block;
  /* margin: 0 auto; */
  text-align: center;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.minl-title .span{
  height:3px;
  width:40px;
  position: absolute;
  bottom: -2px;
  left: 50%;
  margin-left: -20px;
}



.mink {
  width: 100%;
  /* background: #fff; */
  height: 100%;
}

/*右侧栏list的item*/

.nav_right_items {
  /* float: left; */
  width: 49%;
  /* margin: 10rpx 0 5rpx 10rpx; */
  text-align: center;
  color: #4a4a4a;
  background: #fff;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  margin-bottom: 15rpx;
}

.nav_right_items image {
  width: 370rpx;
  height: 380rpx;
  /* margin-top: 15rpx; */
}

.nav_right_items text {
  display: block;
  margin-top: 5rpx;
  margin-bottom: 10rpx;
  /*设置文字溢出部分为...*/
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  text-align: left;
  padding-left: 10rpx;
}

.nav_right_goods_name {
  color: #333;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.nav-bts {
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.prices-gwc image {
  width: 40rpx;
  height: 37rpx;
  padding-right: 10rpx;
}

/** 自定义其他点击态样式类 **/

.other-navigator-hover {
  background: #fff;
}

.scroll_left {
  /* width: 25%; */
  height: 100%;
  background: #fff;
  text-align: center;
  /* position: fixed; */

/**app.wxss**/

page{
  background: #EBEAEA;
}

view{
  min-height: 0;
}

.showLoad{
  position: fixed;
  top: 15%;
  width: 80%;
  height: 770rpx;
  background-color: white;
  left: 10%;
  border-radius: 10rpx;
}
.showLoadModal{
  width: 100%;
  height: 100%;
  position: relative;
  text-align: center;
}
.showLoad_title{
  font-size: 32rpx;
  color: #333333;
  margin-top: 74rpx;
  margin-bottom: 44rpx;
}
.showLoad_tip{
  font-size: 26rpx;
  color: #666666;
  margin-bottom: 14rpx;
}
.showLoad_image{
  margin-top: 40rpx;
}
.showLoad_image image{
  width: 360rpx;
  height: 300rpx;
}
.permite{
  position: absolute;
  right: 60rpx;
  bottom: 250rpx;
  color: #5AA761;
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
  border: 1px solid #e1e1e1;
  background-color: white;
  line-height: 100rpx;
}
.showLoadButton{
  width: 80%;
  margin-top: 40rpx;
  color: white;
  border: none;
}
.fs22{
  font-size: 22rpx;
}
.fs24{
  font-size: 24rpx;
}
.fs26{
  font-size: 26rpx;
}
.fs28{
